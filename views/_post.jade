extends layout

block content
    .container-fluid
        .row
            .col-md-6.col-md-offset-3
                #main.well.post-body
                    h2.post-title= entry.title
                    img.post-image(src=entry.image)
                    .post-content
                        if entry.body
                            != marked(entry.body)

                        iframe(width='100%', height='166', scrolling='no', frameborder='no', src=entry.soundCloudLink)

